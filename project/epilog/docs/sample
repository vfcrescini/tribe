ident sub s1,s2;
ident acc a1;
ident obj o1;
ident sub-grp S1, S2;
ident acc-grp A1;
ident obj-grp O1;

initially holds(s1,a1,o1);
initially memb(s1,S1) && !subst(S1,S2);
initially true;

trans something1(var1, var2) causes holds(var1, a1, var2) if memb(var1, S1) && subst(S1, S2);
trans something2(var3) causes holds(s1, a1, var3) && holds(s2, a1, var3) if true;

/* /* nested comment */
/* comment1 */   /* comment2 */
