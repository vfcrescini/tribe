/*
 * subset test
 * Vino Crescini  <jcrescin@cit.uws.edu.au>
 */

ident sub     E1, E2, E3, E4, E5, E6;
ident sub-grp G1, G2, G3;

initially memb(E1, G1) && memb(E2, G1) && memb(E3, G1);
initially memb(E1, G3) && memb(E2, G3) && memb(E4, G3) && memb(E5, G3);
initially subst(G2, G3);
initially memb(E3, G2);

query subst(G1, G3);
