/*
 * geography
 * Vino Crescini  <jcrescin@cit.uws.edu.au>
 */

/* identifier declarations */

ident sub-grp sydney, melbourne, kingswood, penrith, nsw, victoria, 
              australia, blacktown;

/* initial state */

initially subst(sydney, nsw);
initially subst(nsw, australia);
initially !subst(sydney, victoria);
initially subst(blacktown, sydney);
initially subst(melbourne, victoria);
initially subst(kingswood, penrith);
initially !subst(penrith, melbourne);

/* queries */

query subst(sydney, australia);
query subst(sydney, nsw);
query subst(blacktown, australia);
query subst(melbourne, nsw);
query subst(melbourne, victoria);
query subst(blacktown, victoria);
query subst(sydney, victoria);
