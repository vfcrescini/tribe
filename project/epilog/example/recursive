/*
 * Recursive Example
 * Vino Crescini  <jcrescin@cit.uws.edu.au>
 */

/* identifier declarations */
ident sub-grp a,b,c;

/* initial state */
initially subst(a,b);
initially subst(b,c);
initially subst(c,a);

/* queries */
query subst(a,a);
query subst(a,b);
query subst(a,c);
query subst(b,a);
query subst(b,b);
query subst(b,c);
query subst(c,a);
query subst(c,b);
query subst(c,c);
